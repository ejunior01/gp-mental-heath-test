generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Status {
  IN_PROCESS
  COMPLETED
  FAILURE
}

model SurveyReport {
  id             Int       @id @default(autoincrement())
  totalRecords   Int?
  status         Status    @default(IN_PROCESS)
  filepath       String
  filepathResult String?
  createdAt      DateTime  @default(now())
  uptadedAt      DateTime?
}

model Survey {
  code      String   @id
  noteOne   Int
  notaTwo   Int
  result    Decimal
  createdAt DateTime @default(now())
}
